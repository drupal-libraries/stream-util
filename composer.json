{
    "name": "drupal-libraries/stream-util",
    "type": "library",
    "description": "Helper utilities for dealing with streams. Forked and updated from twistor/stream-util to work with Flysystem 3.0+",
    "license": "MIT",
    "homepage": "http://github.com/drupal-libraries/stream-util",
    "authors": [
        {
            "name": "Chris Leppanen",
            "email": "chris.leppanen@gmail.com"
        },
        {
            "name": "Lisa Ridley",
            "email": "lisa@codementality.com"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0",
        "phpstan/phpstan": "^1.10",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "DrupalLibraries\\": "src/"
        }
    },
    "scripts": {
        "phpcs": "\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility",
        "phpstan": "vendor/bin/phpstan"
    }
}
